{
  "name": "statistic",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "userInfo": {
      "type": "object",
      "required": true
    },
    "destination": {
      "type": "object",
      "required": true
    },
    "method": {
      "type": "string",
      "required": true
    },
    "reduction": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "date": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "agencyRel": {
      "type": "belongsTo",
      "model": "agency",
      "foreignKey": "agencyId"
    },
    "contentRel": {
      "type": "belongsTo",
      "model": "content",
      "foreignKey": "contentId"
    }
  },
  "acls": [],
  "methods": {}
}
